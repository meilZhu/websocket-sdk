{
  "name": "@zmy/websocket-sdk",
  "version": "0.0.3",
  "description": "websocket请求协议的封装",
  "main": "index.js",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "clean:test": "rimraf coverage",
    "clean": "npm run clean:test",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production  webpack --bail --progress --color --env production",
    "serve:dist": "npm run serve --dist",
    "serve": "cross-env NODE_ENV=development node bin/serve",
    "mock": "cross-env NODE_ENV=development node bin/serve_mock",
    "jnlp": "cross-env NODE_ENV=development node bin/serve_jnlp",
    "start": "npm run serve",
    "webdriver": "webdriver-manager update",
    "e2e": "npm run protractor -- --production-backend --suit jenkens",
    "protractor": "npm run webdriver && node ./protractor/e2e",
    "karma": "npm run cean:test",
    "lint": "npm run eslint",
    "test": "npm run karma && npm run e2e",
    "eslint": "eslint --fix  ./protractor ./webpack ./sdk",
    "publish2npm": "npm run build && npm publish --registry=https://registry.npmjs.org/"
  },
  "author": "manyao.zhu",
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.19.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^5.2.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-promise": "^3.5.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "imagemin-webpack-plugin": "^1.4.4",
    "moment": "^2.24.0",
    "opn": "^5.5.0",
    "promise-polyfill": "^6.0.2",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.25.0",
    "wolfy87-eventemitter": "^5.2.9",
    "yargs": "^8.0.2"
  }
}
